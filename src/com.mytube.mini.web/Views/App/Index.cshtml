@model IEnumerable<com.mytube.mini.web.Models.Videos>
@{
    ViewBag.Title = "Home";
    Layout = "_Layout";
}

<h1>Hello my tube!!</h1>
<p>This will be a fun website soon</p>

<form>
    <h5>Sort by date/popularity</h5>
    <ul>
        @foreach (var item in Model)
        {
            <li>@item.VideoName : @item.AddedDate.ToString("d")</li>
        }
    </ul>
</form>
